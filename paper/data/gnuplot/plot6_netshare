reset
dx=5.
n=2
total_box_width_relative=0.75
gap_width_relative=0.1
d_width=(gap_width_relative+total_box_width_relative)*dx/2.
reset
set term png truecolor
set xlabel "Year"
set ylabel "Profit(Million Dollars)"
set grid
set boxwidth total_box_width_relative/n relative
plot "D:/android/JobShare/paper/data/gnuplot/energy_netshare.dat" u 1:1 w boxes lc rgb"green" notitle,\
     "" u ($1+d_width):2 w boxes lc rgb"red" notitle,