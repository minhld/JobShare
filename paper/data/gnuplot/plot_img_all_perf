# set terminal png transparent nocrop enhanced size 450,320 font "arial,8" 
# set output 'datastrings.5.png'
set style fill   solid 1.00 border lt -1
set grid nopolar
set key inside right top vertical Right noreverse enhanced autotitle columnhead nobox
set pointsize 2
set ylabel "Energy Consumption (uAh)" 
set xtics font "Verdana,12"
set ytics font "Verdana,12"
set yrange [ 500 : 14000 ] noreverse nowriteback
set bmargin  3
plot 'D:/android/JobShare/paper/data/gnuplot/data_largeimg_time.dat' using 1 with linespoints ls 1 lw 2 ps 1 lc rgb "black" title "Cluster with 1 peer", \
'' using 2 with linespoints ls 2 lw 2 ps 1 lc rgb "blue" title "Cluster with 2 peers", \
'' using 3 with linespoints ls 3 lw 2 ps 1 lc rgb "orange" title "Cluster with 3 peers", \
'' using 4 with linespoints ls 3 lw 2 ps 1 lc rgb "green" title "Cluster with 4 peers", \
'' using 5 with linespoints ls 3 lw 2 ps 1 lc rgb "yellow" title "Cluster with 4 peers and more"